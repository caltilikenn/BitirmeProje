<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="followself.com.bitirmeprojesi">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/heart"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/heart"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".YeniUyelik" />
        <activity android:name=".Anasayfa" />
        <activity
            android:name=".YonetimPaneli"
            android:label="Üye Listesi"></activity>
        <activity android:name=".SifremiUnuttum" />
        <activity
            android:name=".Profil"
            android:label="Profil"></activity>
        <activity
            android:name=".HesapAyarlari"
            android:label="Hesap Ayarları"></activity>
        <activity
            android:name=".Olcumler"
            android:label="Ölçümler"></activity>
        <activity
            android:name=".SaglikGecmisi"
            android:label="Sağlık Geçmişi"></activity>
        <activity
            android:name=".Belgeler"
            android:label="Belgeler"></activity>
        <activity android:name=".Boy"></activity>
    </application>

</manifest>